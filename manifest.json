{
  "name": "FitLog - Logística 100% Digital",
  "short_name": "FitLog",
  "description": "Empresa líder em logística digital com soluções TMS e WMS. Transporte aéreo, rodoviário, armazenagem, eventos e AOG.",
  "version": "1.3.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#1B365D",
  "theme_color": "#E31E24",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "pt-BR",
  "dir": "ltr",
  "categories": ["business", "logistics", "transportation"],
  "icons": [
    {
      "src": "assets/images/fitlog-logo.jpg",
      "sizes": "192x192",
      "type": "image/jpeg",
      "purpose": "any maskable"
    },
    {
      "src": "assets/images/fitlog-logo.jpg",
      "sizes": "512x512",
      "type": "image/jpeg",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "assets/images/IMAGEM NOVA.jpg",
      "sizes": "1280x720",
      "type": "image/jpeg",
      "form_factor": "wide",
      "label": "Página inicial do FitLog"
    }
  ],
  "shortcuts": [
    {
      "name": "Nossos Serviços",
      "short_name": "Serviços",
      "description": "Conheça nossos serviços logísticos",
      "url": "/#services",
      "icons": [
        {
          "src": "assets/images/fitlog-logo.jpg",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Contato",
      "short_name": "Contato",
      "description": "Entre em contato conosco",
      "url": "/#contact",
      "icons": [
        {
          "src": "assets/images/fitlog-logo.jpg",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "capture_links": "new-client",
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/contact?email=%s"
    },
    {
      "protocol": "tel",
      "url": "/contact?phone=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*", "application/pdf", ".doc", ".docx"]
        }
      ]
    }
  }
}
